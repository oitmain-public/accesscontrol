{
  "name": "accesscontrol",
  "version": "1.5.0",
  "description": "Role and Attribute based Access Control for Node.js",
  "main": "index.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "files": [
    "index.js",
    "lib",
    "LICENSE"
  ],
  "types": "lib/AccessControl",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && mkdirp ./lib && tsc",
    "dist": "npm run build && mkdirp ./dist && ./node_modules/.bin/browserify lib/AccessControl.js --standalone AccessControl | derequire > dist/accesscontrol.js && echo '✔ Assets built!'",
    "dist-min": "npm run dist && cat dist/accesscontrol.js | ./node_modules/.bin/uglifyjs -cm -o dist/accesscontrol.min.js 2> /dev/null && echo '✔ Assets built!'",
    "test": "npm run build && node test",
    "docs": "docma -c ./docma.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onury/accesscontrol.git"
  },
  "keywords": [
    "access",
    "access-control",
    "role",
    "attribute",
    "grant",
    "deny",
    "allow",
    "reject",
    "permission",
    "action",
    "possession",
    "rbac",
    "abac",
    "crud",
    "create",
    "read",
    "update",
    "delete",
    "resource",
    "express",
    "admin",
    "user"
  ],
  "author": "Onur Yildirim <onur@cutepilot.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onury/accesscontrol/issues"
  },
  "homepage": "https://github.com/onury/accesscontrol#readme",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "docma": "^1.5.1",
    "jasmine": "^2.5.3",
    "jasmine-console-reporter": "^1.2.7",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.2.1",
    "uglifyjs": "2.4.10",
    "browserify": "13.0.1",
    "derequire": "2.0.6"
  },
  "dependencies": {
    "notation": "^1.0.0"
  }
}
